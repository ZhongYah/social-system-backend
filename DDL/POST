CREATE TABLE post (post_id int NOT NULL AUTO_INCREMENT, user_id int NOT NULL, content varchar(1024) NOT NULL, image varchar(1024), created_at timestamp, PRIMARY KEY (post_id)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 DEFAULT COLLATE=utf8mb4_0900_ai_ci;
