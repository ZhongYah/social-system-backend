CREATE TABLE user (user_id int NOT NULL AUTO_INCREMENT, user_name varchar(256) NOT NULL, phone varchar(256) NOT NULL, email varchar(256) NOT NULL, password varchar(256) NOT NULL, cover_image varchar(1024), biography varchar(1024) NOT NULL, PRIMARY KEY (user_id), CONSTRAINT phone UNIQUE (phone)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 DEFAULT COLLATE=utf8mb4_0900_ai_ci;
